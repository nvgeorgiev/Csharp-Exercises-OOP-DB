@model IEnumerable<PetListingServiceModel>

<h1>All Pets</h1>

<div>
    <a href="" class="float-left btn btn-info">Prev</a>
    <a href="" class="float-right btn btn-info">Next</a>
</div>

<table class="table table-bordered table-striped">
    <tr>
        <th>
            Type
        </th>
        <th>
            Breed
        </th>
        <th>
            Price
        </th>
    </tr>

    @foreach (var pet in Model)
    {
        <tr>
            <td>
                @pet.Category
            </td>
            <td>
                @pet.Breed
            </td>
            <td>
                $ @pet.Price
            </td>
        </tr>
    }
</table>

